{
  "templates": [
    {
      "label": "product",
      "definition": {
        "numRows": 1000,
        "countries": [
          "de_DE"
        ],
        "rows": [
          {
            "type": "AlphaNumeric",
            "title": "sku",
            "settings": {
              "placeholder": "XXXXXXXXX"
            }
          },
          {
            "type": "Constant",
            "title": "_type",
            "settings": {
              "loopCount": 1,
              "values": "simple"
            }
          },
          {
            "type": "Constant",
            "title": "_attribute_set",
            "settings": {
              "loopCount": 1,
              "values": "Default"
            }
          },
          {
            "type": "Constant",
            "title": "_product_websites",
            "settings": {
              "loopCount": 1,
              "values": "base"
            }
          },
          {
            "type": "TextRandom",
            "title": "name",
            "settings": {
              "startsWithLipsum": false,
              "minWords": 1,
              "maxWords": 4
            }
          },
          {
            "type": "Float",
            "title": "price",
            "settings": {
              "min": 0,
              "max": 1000,
              "decimalPoint": ".",
              "decimalPlaces": 3
            }
          },
          {
            "type": "TextRandom",
            "title": "description",
            "settings": {
              "startsWithLipsum": false,
              "minWords": 20,
              "maxWords": 100
            }
          },
          {
            "type": "TextRandom",
            "title": "short_description",
            "settings": {
              "startsWithLipsum": false,
              "minWords": 5,
              "maxWords": 30
            }
          },
          {
            "type": "NumberRange",
            "title": "weight",
            "settings": {
              "rangeMin": 0,
              "rangeMax": 50
            }
          },
          {
            "type": "Constant",
            "title": "status",
            "settings": {
              "loopCount": 1,
              "values": "1"
            }
          },
          {
            "type": "Constant",
            "title": "visibility",
            "settings": {
              "loopCount": 1,
              "values": "4"
            }
          },
          {
            "type": "Constant",
            "title": "tax_class_id",
            "settings": {
              "loopCount": 1,
              "values": "2"
            }
          },
          {
            "type": "NumberRange",
            "title": "qty",
            "settings": {
              "rangeMin": 0,
              "rangeMax": 30
            }
          },
          {
            "type": "Constant",
            "title": "is_in_stock",
            "settings": {
              "loopCount": 1,
              "values": "1"
            }
          }
        ],
        "export": {
          "type" :"JSON",
          "settings" :{
            "stripWhitespace": false,
            "dataStructureFormat": "simple"
          }
        }
      }
    },
    {
      "label": "category",
      "definition": {
        "numRows": 50,
        "countries": [
          "de_DE"
        ],
        "rows": [
          {
            "type": "AutoIncrement",
            "title": "id",
            "settings": {
              "incrementStart": 1,
              "incrementValue": 1
            }
          },
          {
            "type": "Tree",
            "title": "parent_id",
            "settings": {
              "autoIncRowNum": 1,
              "maxSiblings": 5
            }
          },
          {
            "type": "Constant",
            "title": "_root",
            "settings": {
              "loopCount": 1,
              "values": "Default Category"
            }
          },
          {
            "type": "Category",
            "title": "_category",
            "settings": {
              "fieldName": "name"
            }
          },
          {
            "type": "TextRandom",
            "title": "description",
            "settings": {
              "startsWithLipsum": false,
              "minWords": 1,
              "maxWords": 10
            }
          },
          {
            "type": "Constant",
            "title": "is_active",
            "settings": {
              "loopCount": 1,
              "values": "yes"
            }
          },
          {
            "type": "Constant",
            "title": "include_in_menu",
            "settings": {
              "loopCount": 1,
              "values": "yes"
            }
          },
          {
            "type": "TextRandom",
            "title": "meta_description",
            "settings": {
              "startsWithLipsum": false,
              "minWords": 2,
              "maxWords": 20
            }
          },
          {
            "type": "Constant",
            "title": "available_sort_by",
            "settings": {
              "loopCount": 1,
              "values": "position"
            }
          },
          {
            "type": "Constant",
            "title": "default_sort_by",
            "settings": {
              "loopCount": 1,
              "values": "position"
            }
          }
        ],
        "export": {
          "type" :"JSON",
          "settings" :{
            "stripWhitespace": false,
            "dataStructureFormat": "simple"
          }
        }
      }
    }
  ],
  "postprocessors" : [
    {
      "type": "concat_recursive",
      "object": "category",
      "fieldId": "id",
      "fieldParentId": "parent_id",
      "fieldToConcat": "_category",
      "concatenateChar": "/"
    },
    {
      "type": "remove_fields",
      "object": "category",
      "fieldsToRemove": [
        "id",
        "parent_id"
      ]
    }
  ],
  "relations" : {
    "product": {
      "categories": {
        "foreignObject": "category",
        "foreignField": "_category",
        "pickerSettings": {
          "type": "random",
          "min": 2,
          "max": 5,
          "probability": 70
        }
      }
    }
  }
}
